{
    "name": "safety-platform-extension",
    "displayName": "AI Safety Platform",
    "description": "AI-powered Git Safety and Chatbot for your workflow.",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/bansaldhruv0310/tasksssssssssss"
    },
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "safety-platform.helloWorld",
                "title": "Safety Platform: Hello World"
            },
            {
                "command": "safety-platform.validateBranch",
                "title": "Safety Platform: Validate Current Branch"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "safety-platform-sidebar",
                    "title": "AI Safety",
                    "icon": "media/robot.svg"
                }
            ]
        },
        "views": {
            "safety-platform-sidebar": [
                {
                    "type": "webview",
                    "id": "safety-platform.chatView",
                    "name": "AI Safety Chat",
                    "icon": "media/robot.svg"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "20.2.5",
        "@types/vscode": "^1.80.0",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "@vscode/test-electron": "^2.3.2",
        "eslint": "^8.41.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "@types/node-fetch": "^2.6.13",
        "node-fetch": "^2.7.0"
    }
}